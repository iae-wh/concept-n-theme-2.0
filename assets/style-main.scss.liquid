            }
        }
    }
    .page-table {
        tr {
            border: 1px solid #A3A3A3;
            border-right-width: 0;
            border-left-width: 0;

            &:first-child {
                border-top: none;
            }

            &:last-child {
                border-bottom: none;
            }
        }

        td {
            border: 1px solid #A3A3A3;
            border-top-width: 0;
            border-bottom-width: 0;
            padding: 20px 10px;

            &:first-child{
                border-left: none;
            }

            &:last-child{
                border-right: none;
            }
        }
    }

    .return-policy-pages {
        padding-top: 60px;

        @media(max-width: 767px) {
            padding-top: 40px;
        }
    }

    .terms-of-service-pages, .terms-and-conditions-pages {
        padding-top: 60px;
        padding-bottom: 40px;

        @media(max-width: 767px) {
            padding-top: 40px;
        }
    }

    .contact-us-pages, .promotion-code-pages, .copyright-pages, .privacy-policy-pages {
        padding-top: 60px;

        @media(max-width: 767px) {
            padding-top: 40px;
        }
    }

    .coupon-discount-pages, .about-us-pages {
        padding-top: 60px;

        @media(max-width: 767px) {
            padding-top: 40px;

            .text-content {
                font-size: 16px;

                .text-content_title {
                    font-size: 24px;
                }
            }
        }
    }

    .form-container {
        form {
            display: flex;
            gap: 20px;
            flex-wrap: wrap;

            @media(max-width: 767px) {
                gap: 10px;
                padding-bottom: 40px;
            }

            > div {
                width: 100%;

                &.contact-name , &.contact-email {
                    width: calc(50% - 10px);

                    @media(max-width: 767px) {
                        width: 100%;
                    }
                }
            }

            .alert {
                display: flex;
                gap: 10px;
            }
        }
        .contact-name, .contact-email, .contact-subject {
            input {
                border-radius: 0;
                border: 1px solid #707070;
                width: 100%;
                background-color: transparent;
                padding: 11px 20px;
                font-size: 14px;
                line-height: 1.2;

                &::placeholder {
                    font-size: 14px;
                    color: #B4B4B4;
                    font-weight: 400;
                }
            }
        }

        .contact-message {
            textarea {
                border-radius: 0;
                border: 1px solid #707070;
                width: 100%;
                background-color: transparent;
                padding: 11px 20px;
                font-size: 14px;
                line-height: 1.2;

                &::placeholder {
                    font-size: 14px;
                    color: #B4B4B4;
                    font-weight: 400;
                }
            }
        }

        .contact-submit {
            text-align: right;

            .shop-button {
                display: inline-block;
                color: #231F20;
                padding: 12px 35px;
                border-radius: 0;
                background-color: #fff;
                border: 1px solid #231F20;
                font-size: 14px;
                text-align: center;
                line-height: 1.2;
                letter-spacing: 1.68px;
                font-weight: bold;
                min-width: 150px;

                &:hover, &:focus {
                    background-color: #231F20;
                    color: #fff;
                }

                @media(max-width: 767px) {
                    width: 100%;
                }
            }
        }
    }
}


