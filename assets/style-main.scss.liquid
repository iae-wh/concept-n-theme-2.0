input[type=checkbox] {
    position: relative;
    cursor: pointer;
}

input[type=checkbox]:before {
    content: "";
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    border: 1px solid #000;
    border-radius: 0;
    padding: 1px;
    background-color: #FFFFFF;
}

input[type=checkbox]:checked:before {
    background-color: #000;
}

input[type=checkbox]:checked:after {
    content: "";
    display: block;
    width: 5px;
    height: 9px;
    border: solid white;
    border-width: 0 2px 2px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
    position: absolute;
    top: 1px;
    left: 4px;
}
.container-page-cms {
    max-width: 1006px;
    padding-left: 10px;
    padding-right: 10px;
    display: block;
    margin: auto;
    width: 100%;

    @media(max-width: 767px) {
        padding-left: 20px;
        padding-right: 20px;
    }
}

.page-cms {
    .title-page {
        &.page-has-background {
            display: flex;
            justify-content: flex-start;
            align-items: center;
            width: 100%;
            background-image: var(--bg_breadcrumb);
            aspect-ratio: 1920/277;
            background-size: cover;

            @media(max-width: 767px) {
                background-image: var(--bg_breadcrumb_mobile);
                aspect-ratio: 430/277;
                background-size: cover;
            }
        }

        h2 {
            font-size: 32px;
            color: #000000;
            text-transform: capitalize;
            font-weight: normal;
            padding: 50px 10px;

            img {
                width: 214px;
                padding-left: 5px;
            }

            @media(max-width: 767px) {
                font-size: 24px;
                padding: 20px;

                img {
                    width: 175px;
                }
            }
        }
    }

    .delivery-info-pages {
        padding-top: 50px;

        @media(max-width: 767px) {
            padding-top: 40px;
        }

        .text-content * {
